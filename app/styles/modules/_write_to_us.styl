@keyframes cbh-circle-img-anim{
  0%, 100%, 50%{
    transform:rotate(0) scale(1) skew(1deg) }
  10%, 30%{
    transform:rotate(-25deg) scale(1) skew(1deg) }
  20%, 40%{
    transform:rotate(25deg) scale(1) skew(1deg) }
}
@-moz-keyframes cbh-circle-img-anim{
  0%{
    transform:rotate(0) scale(1) skew(1deg) }
  10%, 30%{
    -moz-transform:rotate(-25deg) scale(1) skew(1deg) }
  20%, 40%{
    -moz-transform:rotate(25deg) scale(1) skew(1deg) }
  100%, 50%{
